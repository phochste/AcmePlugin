{
    "manifest_version": 2,
  
    "name": "AcmeMe",
    "description": "This extension provides an Acme context menu",
    "version": "1.0",
  
    "icons": { "40": "logo.png" },

    "background": {
        "scripts": ["background.js"]
    },

    "action": {
        "default_icon": {                
          "40": "logo.png"
      }
    },

    "permissions": [
        "tabs",
        "contextMenus"
    ]
}