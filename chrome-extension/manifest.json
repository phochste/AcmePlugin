{
  "manifest_version": 2,

  "name": "Acme",
  "description": "This extension provides an Acme context menu",
  "version": "1.0",

  "icons": { "40": "logo.png" },

  "background": {
      "scripts": ["background.js"]
  },

  "options_page": "options.html",

  "permissions": [
      "tabs",
      "contextMenus" ,
      "storage"
  ]
}