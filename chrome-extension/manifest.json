{
  "manifest_version": 3,

  "name": "Acme Open In",
  "description": "This extension provides an Acme context menu",
  "version": "0.0.2",

  "icons": { "40": "logo.png" },

  "background": {
      "service_worker": "background.js",
      "type": "module"
  },

  "options_page": "options.html",

  "permissions": [
      "tabs",
      "contextMenus" ,
      "storage"
  ]
}